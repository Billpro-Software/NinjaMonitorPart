<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<com>
  <teliasonera>
    <ipl>
      <ninja>
        <db>
          <!-- The class-name of the JDBC driver -->
          <driver>oracle.jdbc.OracleDriver</driver>
          <!-- The sub-protocol, for example 'oracle:thin' or 'oracle:oci8' -->
          <subprotocol>oracle:thin</subprotocol>
          <!-- The Database URL, formatted as such: 'host:port:sid' -->
          <url>(DESCRIPTION=(CONNECT_TIMEOUT=1)(ADDRESS_LIST=(ADDRESS=(PROTOCOL=TCP)(HOST=aa161.netcom.no)(PORT=1521))(ADDRESS=(PROTOCOL=TCP)(HOST=aa146.netcom.no)(PORT=1521)))(CONNECT_DATA=(SERVICE_NAME=NI01PNSRV)))</url>
          <!-- The user / schema name -->
          <username>ninjamaster</username>
          <!-- The password -->
          <password>ninjamaster</password>
          <pool>
            <!-- The minutes of idle until a pool'ed connection is closed -->
            <idletime ignoreempty="true"></idletime>
            <!-- The logging interval - in minutes -->
            <loginterval ignoreempty="true"></loginterval>
            <!-- The (maximum) size of the pool -->
            <size ignoreempty="true"></size>
          </pool>
        </db>
      </ninja>
    </ipl>
  </teliasonera>
</com>
